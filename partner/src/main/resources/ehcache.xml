<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
    <diskStore path="java.io.tmpdir"/>
        <cache name="mycache"
               maxElementsInMemory="1000"
               eternal="false"
               timeToIdleSeconds="30"
               timeToLiveSeconds="30"
               overflowToDisk="true"
        />
        <cache name="user"
               maxElementsInMemory="1000"
               eternal="false"
               timeToIdleSeconds="600"
               timeToLiveSeconds="600"
               overflowToDisk="true"
        />
</ehcache>
